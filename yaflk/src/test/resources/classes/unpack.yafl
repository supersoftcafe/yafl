
# Takes tuple and unpacks into outer tuple to make a new bigger tuple
# Useful on function calls where multiple parameters come from the result of some other call

fun `+`(a, b) = __builtin__ add_i32(a, b)

val oneValue = 13
val twoValues = (13, 27)

fun unpackOne() = `+`(*oneValue, 27)
fun unpackTwo() = `+`(*twoValues)

fun main() = unpackOne() + unpackTwo()
